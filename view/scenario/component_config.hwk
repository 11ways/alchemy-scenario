<% makeDialog() %>
<% addClass('component-config-dialog') %>

<div class="component-config-wrapper js-he-ready-ccw">
	<al-form
		#document={% component_settings %}
		#schema={% schema %}
		class="main-form"
	>
		{% each schema as field %}
			<al-field
				field-name={% field.name %}
			></al-field>
		{% /each %}
	</al-form>

	<button class="btn btn-apply">
		Apply
	</button>
</div>