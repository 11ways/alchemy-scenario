<% style('element/flow') %>

{# Placeholder if, should implement! #}

{% if show_schema_instead %}
	<al-field-schema
		#alchemy_field=<% alchemy_field %>
		#field_context=<% field_context %>
	></al-field-schema>
{% else %}
	<al-flow
		class="alchemy-field-value"
		component-category=<% alchemy_field.config.options.category %>
		#alchemy_field=<% alchemy_field %>
		#field_context=<% field_context %>
		#value=<% value %>
	></al-flow>
{% /if %}